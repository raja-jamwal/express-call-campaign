{
  "name": "express-call-campaign",
  "version": "1.0.0",
  "description": "Express + Prisma + BullMQ boilerplate for call campaigns",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "worker:call": "ts-node-dev --respawn --transpile-only src/workers/callTaskWorker.ts",
    "worker:call-scheduler": "ts-node-dev --respawn --transpile-only src/workers/callSchedulerWorker.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:pull": "prisma db pull",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "seed": "ts-node scripts/seeder.ts",
    "example": "ts-node examples/api-client.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "express",
    "prisma",
    "bullmq",
    "typescript"
  ],
  "author": "Raja Jamwal",
  "license": "MIT",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@bull-board/api": "^6.14.2",
    "@bull-board/express": "^6.14.2",
    "@bull-board/ui": "^6.14.2",
    "@prisma/client": "^6.19.0",
    "bullmq": "^5.0.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "ioredis": "^5.4.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@jest/globals": "^30.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.0.0",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^30.2.0",
    "prisma": "^6.19.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
